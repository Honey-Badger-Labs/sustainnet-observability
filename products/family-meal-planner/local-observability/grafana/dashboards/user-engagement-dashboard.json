{
  "dashboard": {
    "id": null,
    "title": "Family Meal Planner - User Engagement",
    "tags": ["family-meal-planner", "users", "engagement", "business"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Users",
        "type": "stat",
        "targets": [
          {
            "expr": "database_total_users{job=\"family-meal-planner-api\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Daily Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "count(count by (user_id) (auth_login_success_total{job=\"family-meal-planner-api\"}[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 4,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Active Families",
        "type": "stat",
        "targets": [
          {
            "expr": "database_total_families{job=\"family-meal-planner-api\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 8,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Login Success Rate (%)",
        "type": "gauge",
        "targets": [
          {
            "expr": "(sum(rate(auth_login_success_total{job=\"family-meal-planner-api\"}[24h])) / sum(rate(auth_login_attempts_total{job=\"family-meal-planner-api\"}[24h]))) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "User Growth (Last 7 Days)",
        "type": "graph",
        "targets": [
          {
            "expr": "database_total_users{job=\"family-meal-planner-api\"}",
            "refId": "A",
            "legendFormat": "Total Users"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 6,
        "title": "Daily Active Users Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "count(count by (user_id) (auth_login_success_total{job=\"family-meal-planner-api\"}[24h]))",
            "refId": "A",
            "legendFormat": "DAU"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 7,
        "title": "Recipe Searches (Last 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(recipe_searches_total{job=\"family-meal-planner-api\"}[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 8,
        "title": "Meal Plans Created (Last 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(meal_plans_created_total{job=\"family-meal-planner-api\"}[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 4,
          "y": 12
        }
      },
      {
        "id": 9,
        "title": "Family Invitations Sent (Last 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(family_invitations_sent_total{job=\"family-meal-planner-api\"}[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 8,
          "y": 12
        }
      },
      {
        "id": 10,
        "title": "Invitation Acceptance Rate (%)",
        "type": "gauge",
        "targets": [
          {
            "expr": "(sum(increase(family_invitations_accepted_total{job=\"family-meal-planner-api\"}[7d])) / sum(increase(family_invitations_sent_total{job=\"family-meal-planner-api\"}[7d]))) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 12,
          "y": 12
        }
      },
      {
        "id": 11,
        "title": "Top Features Used (Last 7 Days)",
        "type": "piechart",
        "targets": [
          {
            "expr": "topk(5, sum by (feature) (increase(feature_usage_total{job=\"family-meal-planner-api\"}[7d])))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 12,
        "title": "User Retention (7-Day)",
        "type": "graph",
        "targets": [
          {
            "expr": "(count(count by (user_id) (auth_login_success_total{job=\"family-meal-planner-api\"}[7d])) / count(count by (user_id) (auth_login_success_total{job=\"family-meal-planner-api\"}[14d]))) * 100",
            "refId": "A",
            "legendFormat": "7-Day Retention Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "yaxes": [
          {
            "format": "percent"
          }
        ]
      }
    ],
    "refresh": "1m",
    "time": {
      "from": "now-7d",
      "to": "now"
    }
  }
}
