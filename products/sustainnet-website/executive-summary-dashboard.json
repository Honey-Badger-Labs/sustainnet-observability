{
  "dashboard": {
    "id": null,
    "title": "SustainNet Website - Executive Summary",
    "tags": ["sustainnet", "executive", "business"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Website Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"sustainnet-website\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "ONLINE",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Monthly Visitors",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(cloudfront_requests_total[30d])) * 30 * 24 * 3600",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Average Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(cloudfront_origin_latency_average)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Error Rate (4xx/5xx)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(cloudfront_4xx_error_rate[5m])) + sum(rate(cloudfront_5xx_error_rate[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Daily Visitors Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(cloudfront_requests_total[1d]))",
            "refId": "A",
            "legendFormat": "Daily Requests"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 6,
        "title": "Response Time Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "cloudfront_origin_latency_average",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 7,
        "title": "Top Pages by Traffic",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(rate(cloudfront_requests_by_uri[7d])) by (uri))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "auto"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 8,
        "title": "Geographic Distribution",
        "type": "worldmap",
        "targets": [
          {
            "expr": "sum(rate(cloudfront_requests_by_country[1h])) by (country)",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        }
      }
    ],
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5m",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}